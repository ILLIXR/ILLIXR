site_name: ILLIXR Documentation
theme:
  name: material
  custom_dir: @MKDOCS_DOCS_DIR@/overrides
  palette:
    # Light Mode
    - media: "(prefers-color-scheme: light)"
      scheme: default
      toggle:
        icon: material/weather-night
        name: Light mode
      primary: custom
      accent: custom
    # Dark Mode
    - media: "(prefers-color-scheme: dark)"
      scheme: slate
      toggle:
        icon: material/weather-sunny
        name: Dark mode
      primary: custom
      accent: custom
  features:
    - search.suggest
    - content.code.annotate
    - content.footnote.tooltips
    - navigation.indexes
    - toc.integrate
  language: en
site_dir: @MKDOCS_BUILD_DIR@
site_url: https://illixr.github.io/ILLIXR/docs
markdown_extensions:
  - fenced_code
  - footnotes
  - admonition
  - pymdownx.highlight:
      use_pygments: true
      pygments_lang_class: true
      line_spans: __span
  - pymdownx.inlinehilite
  - pymdownx.snippets
  - pymdownx.superfences
extra_css:
  - css/mkd-extra.css
  - css/code-box-copy.css
extra_javascript:
  - js/jquery.min.js
  - js/clipboard.min.js
  - js/code-box-copy.js
extra:
  version:
    default: latest
    provider: mike
plugins:
  - search
  - mike:
      alias_type: symlink
      redirect_template: null
      deploy_prefix: ''
      canonical_version: latest
      version_selector: true
      css_dir: css
      javascript_dir: js
docs_dir: @MKDOCS_DOCS_DIR@
