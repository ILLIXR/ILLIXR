site_name: ILLIXR Documentation
theme:
  name: material
  custom_dir: @MKDOCS_DOCS_DIR@/overrides
  palette:
    # Light Mode
    - media: "(prefers-color-scheme: light)"
      scheme: default
      toggle:
        icon: material/weather-night
        name: Light mode
      primary: custom
      accent: custom
    # Dark Mode
    - media: "(prefers-color-scheme: dark)"
      scheme: slate
      toggle:
        icon: material/weather-sunny
        name: Dark mode
      primary: custom
      accent: custom
  features:
    - search.suggest
    - content.code.annotate
    - content.footnote.tooltips
    - navigation.indexes
  language: en
site_dir: @MKDOCS_BUILD_DIR@
site_url: https://illixr.github.io/ILLIXR
markdown_extensions:
  - fenced_code
  - footnotes
  - admonition
  - pymdownx.highlight:
      use_pygments: true
      pygments_lang_class: true
      line_spans: __span
  - pymdownx.inlinehilite
  - pymdownx.snippets
  - pymdownx.superfences
  - md_in_html
  - attr_list
extra_css:
  - css/mkd-extra.css
  - css/code-box-copy.css
extra_javascript:
  - js/jquery.min.js
  - js/clipboard.min.js
  - js/code-box-copy.js@MIKE_VERSIONING@
plugins:
  - search@MKDOCS_MIKE@
  - mkdoxy:
      save-api: @MKDOCS_DOCS_DIR@
      projects:
        ILLIXR_API:
          src-dirs: @DOXYGEN_SOURCE_DIR@
          api-path: ../api
          full-doc: True
          doxy-cfg-file: @MKDOCS_DOXY_CFG@
  - include-markdown:
      encoding: ascii
      opening_tag: "{!"
      closing_tag: "!}"
  - glightbox:
      touchNavigation: true
      effect: zoom
      slide_effect: zoom
      zoomable: true
      draggable: false
      background: none
      shadow: false
      manual: true
      caption_position: bottom
docs_dir: @MKDOCS_DOCS_DIR@
